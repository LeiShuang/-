<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/color_child_bg"
    android:layout_width="match_parent"
    android:layout_height="136dp">

        <RelativeLayout
            android:id="@+id/rl_one"
            android:background="@color/colorWhite"
            android:layout_width="104dp"
            android:layout_height="136dp">
            
            <ImageView
                android:id="@+id/iv_head_one"
                android:src="@mipmap/ic_launcher"
                android:scaleType="fitXY"
                android:layout_width="104dp"
                android:layout_height="96dp" />

            <TextView
                android:id="@+id/tv_apply_one"
                android:layout_marginTop="8dp"
                android:layout_marginRight="6dp"
                android:layout_below="@+id/iv_head_one"
                android:layout_alignRight="@+id/iv_head_one"
                android:background="@drawable/background_text_apply"
                android:textColor="#FA857C"
                android:gravity="center"
                android:text="@string/apply_for"
                android:layout_width="48dp"
                android:layout_height="22dp" />

            <TextView
                android:id="@+id/tv_price_one"
                tools:text="价格"
                android:textSize="14sp"
                android:layout_marginLeft="6dp"
                android:layout_alignBaseline="@+id/tv_apply_one"
                android:textColor="#CBCBCB"
                android:layout_below="@+id/iv_head_one"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            
            
        </RelativeLayout>

