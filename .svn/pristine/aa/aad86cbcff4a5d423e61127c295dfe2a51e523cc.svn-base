<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.zfsoft.zfsoft_product.widget.ArcContainer
        app:orignalHeight="277dp"
        android:layout_width="match_parent"
        android:layout_height="300dp">
        <com.youth.banner.Banner
            android:layout_width="match_parent"
            android:layout_height="300dp"
            android:id="@+id/home_banner"/>
    </com.zfsoft.zfsoft_product.widget.ArcContainer>


    <RelativeLayout
        android:layout_marginTop="37dp"
        android:id="@+id/rl_home_search_container"
        android:focusableInTouchMode="true"
        android:focusable="true"
        android:background="@drawable/background_home_search"
        android:layout_marginLeft="@dimen/common_margin_left"
        android:layout_marginRight="@dimen/common_margin_left"
        android:layout_width="match_parent"
        android:layout_height="30dp">
        <!--<EditText
            android:id="@+id/et_home_search"

            android:background="@null"
            android:layout_width="match_parent"
            android:layout_height="30dp" />-->

        <ImageView
            android:src="@mipmap/ico_search"
            android:layout_marginLeft="6dp"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    </RelativeLayout>



</RelativeLayout>