<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:paddingLeft="@dimen/fivteen_padding"
        android:paddingRight="@dimen/nineteen_padding">

        <ImageView
            android:id="@+id/iv_report_detail_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:src="@mipmap/ico_back" />
        <ImageView
            android:id="@+id/iv_try_use_like"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@mipmap/ico_report_like"/>
        <android.support.design.widget.TabLayout
            android:layout_marginBottom="@dimen/five_padding"
            android:id="@+id/try_detail_tabLayout"
            android:layout_toLeftOf="@+id/iv_try_use_like"
            android:layout_toRightOf="@+id/iv_report_detail_back"
            android:layout_marginLeft="@dimen/ten_padding"
            android:layout_marginRight="@dimen/ten_padding"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </RelativeLayout>
    <android.support.v4.view.ViewPager
        android:id="@+id/try_user_detail_viewPager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</LinearLayout>